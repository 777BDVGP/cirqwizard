<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import org.cirqwizard.fx.controls.*?>
<?import org.cirqwizard.fx.controls.RealNumberTextField?>
<?scenebuilder-classpath-element ../../../../../out/production/cirqwizard?>

<StackPane fx:id="view" styleClass="main-pane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <AnchorPane id="AnchorPane" fx:id="regularPane">
            <children>
                <Label layoutX="50.0" layoutY="40.0" styleClass="h1" text="Placing component"/>
                <Label fx:id="header" layoutX="50.0" layoutY="80.0" styleClass="h1" text="Placing component"/>
                <HBox id="HBox" alignment="CENTER" spacing="5.0" AnchorPane.bottomAnchor="28.0"
                      AnchorPane.rightAnchor="37.0">
                    <children>
                        <Button id="continueButton" defaultButton="true" mnemonicParsing="false"
                                onAction="#next" text="Continue"/>
                    </children>
                </HBox>
                <HBox id="HBox" alignment="CENTER" layoutX="53.0" layoutY="150.0" spacing="5.0">
                    <children>
                        <Label text="Component being placed"/>
                        <ComboBox fx:id="componentName" prefWidth="200.0"/>
                    </children>
                </HBox>
                <TitledPane animated="false" collapsible="false" layoutY="40.0" text="Target"
                            AnchorPane.rightAnchor="20.0">
                    <content>
                        <GridPane hgap="5" vgap="5">
                            <children>
                                <Label text="X" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
                                <Label text="Y" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
                                <Label text="Angle" GridPane.columnIndex="0" GridPane.rowIndex="2"/>
                                <RealNumberTextField fx:id="targetX" disable="true"
                                                     GridPane.columnIndex="1" GridPane.rowIndex="0"/>
                                <RealNumberTextField fx:id="targetY" disable="true"
                                                     GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                                <RealNumberTextField fx:id="targetAngle" disable="true"
                                                     GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                            </children>
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="60.0"/>
                                <ColumnConstraints fillWidth="false" hgrow="SOMETIMES" minWidth="10.0"
                                                   prefWidth="85.0"/>
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints vgrow="SOMETIMES"/>
                                <RowConstraints vgrow="SOMETIMES"/>
                                <RowConstraints vgrow="SOMETIMES"/>
                            </rowConstraints>
                        </GridPane>
                    </content>
                </TitledPane>
                <HBox alignment="TOP_CENTER" layoutX="274.0" layoutY="235.0" AnchorPane.leftAnchor="0.0"
                      AnchorPane.rightAnchor="0.0">
                    <children>
                        <TitledPane fx:id="placementPane" animated="false" collapsible="false" minWidth="164.0"
                                    text="Placement offsets">
                            <content>
                                <VBox id="VBox" alignment="CENTER" spacing="10.0">
                                    <children>
                                        <GridPane vgap="5">
                                            <children>
                                                <Label text="X" GridPane.columnIndex="0"
                                                       GridPane.rowIndex="0"/>
                                                <Label text="Y" GridPane.columnIndex="0"
                                                       GridPane.rowIndex="1"/>
                                                <Label text="Angle" GridPane.columnIndex="0"
                                                       GridPane.rowIndex="2"/>
                                                <RealNumberTextField fx:id="placementX"
                                                                     onAction="#placementLocationUpdated"
                                                                     prefWidth="75.0"
                                                                     GridPane.columnIndex="1"
                                                                     GridPane.rowIndex="0"/>
                                                <RealNumberTextField fx:id="placementY"
                                                                     onAction="#placementLocationUpdated"
                                                                     prefWidth="75.0"
                                                                     GridPane.columnIndex="1"
                                                                     GridPane.rowIndex="1"/>
                                                <RealNumberTextField fx:id="placementAngle"
                                                                     onAction="#placementLocationUpdated"
                                                                     prefWidth="75.0"
                                                                     GridPane.columnIndex="1"
                                                                     GridPane.rowIndex="2"/>
                                            </children>
                                            <columnConstraints>
                                                <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES"
                                                                   minWidth="10.0" prefWidth="40.0"/>
                                                <ColumnConstraints fillWidth="false" hgrow="SOMETIMES"
                                                                   minWidth="10.0" prefWidth="70.0"/>
                                            </columnConstraints>
                                            <rowConstraints>
                                                <RowConstraints minHeight="10.0" prefHeight="30.0"
                                                                vgrow="SOMETIMES"/>
                                                <RowConstraints minHeight="10.0" prefHeight="30.0"
                                                                vgrow="SOMETIMES"/>
                                                <RowConstraints minHeight="10.0" prefHeight="30.0"
                                                                vgrow="SOMETIMES"/>
                                            </rowConstraints>
                                            <VBox.margin>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                            </VBox.margin>
                                        </GridPane>
                                        <Button mnemonicParsing="false" onAction="#place" text="Place"/>
                                    </children>
                                </VBox>
                            </content>
                        </TitledPane>
                    </children>
                </HBox>
                <TitledPane fx:id="pickupPane" animated="false" collapsible="false" layoutY="235.0"
                            text="Pickup" AnchorPane.leftAnchor="20.0" prefWidth="200">
                    <content>
                        <VBox alignment="CENTER" spacing="5" id="Content">
                            <children>
                                <HBox id="HBox" alignment="CENTER" spacing="5.0">
                                    <children>
                                        <Label text="X"/>
                                        <RealNumberTextField fx:id="pickupX" onAction="#pickupLocationUpdated"
                                                             prefWidth="75.0"/>
                                    </children>
                                </HBox>
                                <HBox id="HBox" alignment="CENTER" spacing="5.0">
                                    <children>
                                        <Label text="Y"/>
                                        <RealNumberTextField fx:id="pickupY" onAction="#pickupLocationUpdated"
                                                             prefWidth="75.0"/>
                                    </children>
                                </HBox>
                                <Button fx:id="gotoPickupButton" mnemonicParsing="false"
                                        onAction="#gotoPickup" prefWidth="140.0" text="Go to pickup"/>
                                <Button fx:id="pickupButton" mnemonicParsing="false" onAction="#pickup"
                                        prefWidth="140.0" text="Pickup"/>
                                <Button fx:id="gotoTargetButton" mnemonicParsing="false"
                                        onAction="#gotoTarget" prefWidth="140.0" text="Go to target"/>
                                <Button fx:id="pickupNGoButton" mnemonicParsing="false"
                                        onAction="#pickupAndGo" prefWidth="140.0" text="Pickup&amp;go"/>
                            </children>
                        </VBox>
                    </content>
                </TitledPane>
                <TitledPane animated="false" collapsible="false" layoutY="235.0" text="Manual control"
                            AnchorPane.rightAnchor="20.0">
                    <content>
                        <VBox id="VBox" alignment="CENTER" layoutX="26.0" layoutY="17.0" spacing="15.0">
                            <children>
                                <HBox id="HBox" alignment="CENTER" spacing="5.0">
                                    <children>
                                        <Label text="Z position"/>
                                        <RealNumberTextField fx:id="manualZ" onAction="#manualZUpdated"
                                                             prefWidth="75.0"/>
                                    </children>
                                </HBox>
                                <Button fx:id="moveHeadAwayButton" mnemonicParsing="false"
                                        onAction="#moveHeadAway" prefWidth="160.0"
                                        text="Move head away"/>
                                <Button fx:id="vacuumOffButton" mnemonicParsing="false"
                                        onAction="#vacuumOff" prefWidth="160.0" text="Vacuum off"/>
                            </children>
                        </VBox>
                    </content>
                </TitledPane>
            </children>
        </AnchorPane>
        <ImageView fx:id="microscopeImageView" pickOnBounds="true" preserveRatio="true" VBox.vgrow="ALWAYS"/>
        <AnchorPane fx:id="microscopeControlPane">
            <children>
                <HBox AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                    <children>
                        <Button mnemonicParsing="false" onAction="#hideMicroscopePane" text="Button"/>
                    </children>
                </HBox>
            </children>
        </AnchorPane>
    </children>
</StackPane>
